package sp2darray;

public class maxNum {
	public static void main (String [] args) {
		int[][] num= {{1,10,3,5},{5,8,2,56},{6,34,76,12}};
		
		for (int i=0; i<3; i++) {//col
			int max= num[1][0];
			for (int j=1 ; j<4 ;j++)//row
				max=(num[i][j]> max) ? num[i][j] : max;
			
			System.out.println("Row "+ (i+1)+ ": "+ max);
		}
	}

}
