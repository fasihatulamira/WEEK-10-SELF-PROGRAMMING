package sp2darray;
import java.util.*;
public class inputData {// 1d array
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int[]a=new int[5];
		for(int i=0; i<a.length; i++) {
			System.out.println("insert element at index position " + i);
			a[i]=sc.nextInt();
		}
		for (int i=0; i<a.length; i++) {
			System.out.println("at index postion " + i +" element is "+ a[i]);
			
		}
	}

}
