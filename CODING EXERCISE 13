package sp2darray;

public class SmallestNum {
	public static void main(String [] args) {
		int [][] num= {{47,78,23},{23,12,6}};// initialize data
		int min=num[0][0];
		
		for (int i=0; i<num.length; i++) {
			for (int j=0; j<num[0].length; j++) {
				 if (num[i][j]<min)
                  {
					 min=num[i][j];
				 }
			}
		}
		System.out.println("Smallest element: " +min);
		
		
	}}		
